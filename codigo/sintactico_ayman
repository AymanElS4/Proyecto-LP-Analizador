import ply.yacc as yacc
from primitivos_y_limitadores import tokens
from palabras_reservadas_comentarios import reserved

def p_letdeclarative(p):
    'letdeclarative : LET VARIABLE IGUAL valor SEMICOLON'



def p_error(p):
    print("Syntax error in input!")

# Build the parser
parser = yacc.yacc()

while True:
   try:
       s = input('calc > ')
   except EOFError:
       break
   if not s: continue
   result = parser.parse(s)
   print(result)